O123 sub
  (raster scanning)
  (O123 call [x] [y] [w] [h] [dpi] [overscan] [gap])
  (           1   2   3   4    5     6          7  )
  #11=[25.4 / #5]  (X per col)
  #12=[#11 + #7]   (Y per row)
  #20=0
  G0 X[#1-#6] Y[#2]
  O101 while [#20 lt #4]
    (move to next line)
    G0 Y[#2-#20*#12]
    (forward scan)
    G1 X[#1+#3*#11+#6]
    #20=[#20+1]
    O102 if [#20 lt #4]
      (move to next line)
      G0 Y[#2-#20*#12]
      (backward scan)
      G1 X[#1-#6]
      #20=[#20+1]
    O102 endif
  O101 endwhile
O123 endsub
M2
